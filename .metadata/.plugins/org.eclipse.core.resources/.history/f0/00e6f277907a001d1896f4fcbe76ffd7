	//boardList.do
	// HandlerMapping
	@RequestMapping("/boardList.do")
	public String boardList() {
		Board vo = new Board();
		
		
		List<Board> list = new ArrayList<Board>();
		list.add(null);
		
		return "boardList"; //뷰의 이름 /WEB-INF/vies/boardList.jsp로간다.
	}
	
}
